<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.Scene?>
<Scene fx:id="stepModePage" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
       fx:controller="com.example.StepModeController">
    <AnchorPane prefHeight="650.0" prefWidth="900.0">
        <Button fx:id="nextButton" layoutX="244.0" layoutY="575.0" mnemonicParsing="false" onAction="#nextButtonClick"
                prefHeight="50.0" prefWidth="130.0" text="Next"/>
        <Button fx:id="finishButton" layoutX="526.0" layoutY="575.0" mnemonicParsing="false"
                onAction="#finishButtonClick" prefHeight="50.0" prefWidth="130.0" text="Finish"/>
        <ScrollPane fx:id="scrollPane" prefHeight="550.0" prefWidth="904.0">
            <AnchorPane fx:id="diagramPane" prefHeight="534.0" prefWidth="900.0"/>
        </ScrollPane>
    </AnchorPane>
</Scene>
